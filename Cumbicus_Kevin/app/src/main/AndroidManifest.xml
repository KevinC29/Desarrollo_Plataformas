<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cumbicus_kevin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cumbicus_Kevin">
        <activity android:name=".ActivityVolleyAlumno"></activity>
        <activity
            android:name=".ActivityBDDocente"
            android:label="@string/title_activity_b_d_docente"
            android:theme="@style/Theme.Cumbicus_Kevin.NoActionBar" />
        <activity
            android:name=".ActivityMemoriaSD"
            android:label="@string/title_activity_memoria_s_d"
            android:theme="@style/Theme.Cumbicus_Kevin.NoActionBar" />
        <activity
            android:name=".ActivityMemoriaPrograma"
            android:label="@string/title_activity_memoria_programa"
            android:theme="@style/Theme.Cumbicus_Kevin.NoActionBar" />
        <activity
            android:name=".ActividadMemoriaInterna"
            android:label="@string/title_activity_actividad_memoria_interna"
            android:theme="@style/Theme.Cumbicus_Kevin.NoActionBar" />
        <activity
            android:name=".Activity_Principal"
            android:label="@string/title_activity__principal"
            android:theme="@style/Theme.Cumbicus_Kevin.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".ActividadFragmentos" />
        <activity android:name=".ActivityNombres" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>